{
    "name": "telegram-live/streaming",
    "description": "Telegram Live Streaming from YouTube",
    "type": "project",
    "require": {
        "php": ">=8.0",
        "danog/madelineproto": "^8.0",
        "google/apiclient": "^2.15",
        "vlucas/phpdotenv": "^5.5",
        "monolog/monolog": "^3.0"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "TelegramLive\\": "src/"
        }
    },
    "scripts": {
        "post-install-cmd": [
            "php -r \"copy('.env.example', '.env');\""
        ]
    }
}
